{
  "name": "NuTab",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Organize your tabs on cloud",
  "permissions": [
    "alarms",
    "webNavigation",
    "tabs", 
    "chrome://favicon/*", 
    "*://*.google.com/"
  ],
  "content_security_policy": "script-src 'self' https://api.embed.ly; object-src 'self'",
  "background": { 
    "persistent": false,
    "page": "background.html" },
  "browser_action": {
      "default_title": "Show Tabs in Nutab",
      "default_icon": "icon_24.png"
  }
}
