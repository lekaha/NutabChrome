{
  "name": "NuTab",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Organize your tabs on cloud",
  "permissions": [
    "tabs", "chrome://favicon/*", "http://www.google.com/", "https://www.google.com/"
  ],
  "content_security_policy": "script-src 'self' https://api.embed.ly; object-src 'self'",
  "background": { "scripts": ["jquery-2.0.0.min.js", "ready.js"] },
  "background_page": "background.html",
  "browser_action": {
      "default_title": "Show Tabs in Nutab",
      "default_icon": "icon_24.png"
  }
}
